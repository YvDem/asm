{
  "name": "pinguin-asm",
  "author": {
    "name": "Yvan",
    "email": "yampleman1401@gmail.com"
  },
  "publisher": "Yvan",
  "displayName": "pasm",
  "description": "personnal asm syntax support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pasm",
      "aliases": ["pasm", "pinguasm", "pasm"],
      "extensions": [".asm", ".pasm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pasm",
      "scopeName": "source.asm",
      "path": "./syntaxes/pasm.tmLanguage.json"
    }],
    "themes": [
      {
        "label": "pasm++",
        "uiTheme": "vs-dark",
        "path": "./colorthemes/pasm_theme.json"
      }
    ],
    "snippets": [
      {
        "language": "pasm",
        "path": "./snippets/pasm.json"
      }
    ]
  }
}
